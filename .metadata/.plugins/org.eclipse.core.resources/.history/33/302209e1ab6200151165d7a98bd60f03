package nl.jgermeraad;


public class Bridge implements Runnable {
	private Farmer lastFarmer;
	
	public static void main(String[] args) {
		new Bridge().run();
	}
	
	private static class Farmer {
		private String name;
		
		public Farmer(String name) {
			this.name = name;
		}
	}

	@Override
	public void run() {
		lastFarmer = null;
	}
}
